@model IEnumerable<IdentityRole>

@{
	ViewData["Tilte"] = "Roles";
}


<h1 class="container">Roles</h1>
<table class="table table-striped mt-4 container">
	<thead >
		<tr class="bg-primary text-white">
			<th>Id</th>
			<th>Role Name</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var role in Model)
		{
			<tr>
				<td>@role.Id</td>
				<td>@role.Name</td>
			</tr>
		}
	</tbody>
</table>

